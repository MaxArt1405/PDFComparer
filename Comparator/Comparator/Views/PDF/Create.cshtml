@model Comparator.Models.PDFfile

@{
    ViewBag.Title = "Create";
}

<h2>Create</h2>

@using (Html.BeginForm("Create", "PDF", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)

    <fieldset>
        <legend>pdfFile</legend>

        <div class="editor-label">
            Upload File
        </div>
        <div class="editor-field">
            <input type="file" id="file" name="file" />
        </div>
        <p>
            <input type="submit" value="Save" />
        </p>
    </fieldset>
    <p>@Html.ActionLink("Go Back", "Index", "PDF")</p>
}